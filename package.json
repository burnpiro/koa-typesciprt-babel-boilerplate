{
  "name": "koa-typescript-babel-boilerplate",
  "version": "1.0.0",
  "description": "API for blog app",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon --exec babel-node --extensions '.ts,.js' src/index.ts",
    "build": "webpack --config config/webpack.config.js",
    "start-prod": "node dist/server.js",
    "db-migration": "migrate",
    "test": "jest"
  },
  "keywords": [
    "koajs",
    "koa",
    "node",
    "api",
    "mongodb",
    "javascript",
    "typescript"
  ],
  "author": "Kemal Erdem",
  "license": "ISC",
  "dependencies": {
    "@babel/register": "^7.0.0",
    "dotenv": "^6.1.0",
    "koa": "^2.6.2",
    "koa-mount": "^4.0.0",
    "migrate": "^1.6.2",
    "mongodb": "3.1.13",
    "mongoose": "^5.4.12",
    "slug": "^0.9.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/dotenv": "^6.1.0",
    "@types/koa": "^2.0.48",
    "@types/mongodb": "^3.1.19",
    "@types/mongoose": "^5.3.17",
    "@types/node": "^10.12.26",
    "@types/slug": "^0.9.1",
    "babel-loader": "^8.0.5",
    "chokidar": "^2.1.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "tslint": "^5.12.1",
    "tslint-config-airbnb-base": "^0.2.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  }
